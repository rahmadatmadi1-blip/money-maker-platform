{
  "name": "money-maker-platform",
  "version": 2,
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/.*",
      "dest": "/build/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://money-maker-backend.onrender.com",
    "REACT_APP_CLIENT_URL": "https://money-maker-platform.vercel.app"
  }
}