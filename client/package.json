{
  "name": "money-maker-client",
  "version": "1.0.0",
  "description": "Frontend client for Money Maker Platform",
  "homepage": ".",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.5.0",
    "chart.js": "^4.4.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.279.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.4",
    "react-toastify": "^9.1.3",
    "socket.io-client": "^4.7.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "npm run build && npx serve -s build -l 3001 & sleep 3 && npx webpack-bundle-analyzer build/static/js/*.js",
    "analyze:size": "npm run build && du -sh build/* | sort -hr",
    "performance:test": "npm run test -- --coverage --watchAll=false --testPathPattern=performance",
    "lighthouse": "npm run build && npx serve -s build -l 3001 & sleep 5 && npx lighthouse http://localhost:3001 --output=html --output-path=./lighthouse-report.html",
    "build:analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
