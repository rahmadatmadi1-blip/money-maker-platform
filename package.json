{
  "name": "money-maker-platform",
  "version": "1.0.0",
  "description": "Platform monetisasi online dengan multiple revenue streams",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server.js",
    "deploy:frontend": "cd client && npm run build && vercel --prod",
    "deploy:backend": "git add . && git commit -m 'Deploy to Render' && git push origin main",
    "deploy:all": "npm run build && npm run deploy:frontend && npm run deploy:backend",
    "deploy:prepare": "node deploy.js",
    "deploy:help": "node deploy.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "monetization",
    "affiliate",
    "ecommerce",
    "marketplace",
    "revenue"
  ],
  "author": "Money Maker Platform",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.9.4",
    "socket.io": "^4.7.2",
    "stripe": "^13.6.0",
    "validator": "^13.11.0",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
